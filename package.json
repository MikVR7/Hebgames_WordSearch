{
  "name": "wordsearch",
  "version": "1.0.0",
  "description": "A customizable WordSearch game for web browsers and mobile devices",
  "main": "index.html",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc && npm run copy-files && npm run copy-lib",
    "build:ts": "tsc",
    "copy-files": "copyfiles -u 1 \"src/**/*.html\" \"src/**/*.css\" \"src/**/*.svg\" \"src/**/*.jpg\" \"src/**/*.jpeg\" \"src/**/*.png\" dist && copyfiles index.html dist",
    "copy-lib": "copyfiles -u 1 \"lib/**/*\" dist/lib",
    "start": "npm run build && http-server dist -p 5500",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "word search",
    "puzzle game",
    "browser game",
    "mobile game",
    "educational game"
  ],
  "author": "Tokele",
  "license": "MIT",
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "http-server": "^14.1.1",
    "rimraf": "^5.0.10",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "aws-sdk": "^2.1679.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MikVR7/Hebgames_WordSearch.git"
  },
  "bugs": {
    "url": "https://github.com/MikVR7/Hebgames_WordSearch/issues"
  },
  "homepage": "https://github.com/MikVR7/Hebgames_WordSearch#readme"
}
