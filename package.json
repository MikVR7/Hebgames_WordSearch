{
  "name": "wordsearch",
  "version": "1.0.0",
  "description": "A customizable WordSearch game for web browsers and mobile devices",
  "main": "index.html",
  "scripts": {
    "build": "tsc && npm run copy-files",
    "copy-files": "copyfiles -u 1 src/**/*.html src/**/*.css dist/",
    "start": "npm run build && http-server dist"
  },
  "keywords": ["word search", "puzzle game", "browser game", "mobile game", "educational game"],
  "author": "Tokele",
  "license": "MIT",
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "typescript": "^5.5.2",
    "http-server": "^14.1.1"
  }
}